<Window x:Class="UserInterface.Windows.FieldInfoUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UserInterface.Windows"
        mc:Ignorable="d"
        Title="FieldInfoUI" Height="165" Width="300">
    <Grid>
        <StackPanel>
            <Label Content="Pasirinkite norimą lauką" HorizontalAlignment="Center" FontSize="20" Margin="10"/>
            <ComboBox x:Name="fieldInfoBox" Margin="10"/>
            <Button Content="Pasirinkti" Height="30" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
